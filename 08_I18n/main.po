#: main.c:24
#, c-format
msgid "Pick a random number between 1 and a 100\n"
msgstr "Выберите случайное число от 1 до 100\n"

#: main.c:31
#, c-format
msgid "Is your number larger than %d? (yes/no)\n"
msgstr "ваше число больше %d? (да/нет)\n"

#: main.c:34
msgid "yes"
msgstr "да"

#: main.c:36
msgid "no"
msgstr "нет"

#: main.c:39
#, c-format
msgid "Please, enter yes or no\n"
msgstr "Пожалуйста, введите да или нет\n"

#: main.c:42
#, c-format
msgid "You've picked number %d!\n"
msgstr "Вы выбрали число %d!\n"
